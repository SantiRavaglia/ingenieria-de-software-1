!Perforadora class methodsFor: 'instance creation' stamp: 'aa 5/8/2023 21:37:21' prior: 50613088!
noHayDosCapasDeSueloContiguas: capasDeSueloAExcavar 
	
	| capaAnterior capaActual |
	
	self extraerCapasDeSuelo: capasDeSueloAExcavar .

	capasDeSueloAExcavar size > 1 ifTrue: [
		(2 to: capasDeSueloAExcavar size) do: [ :index |
			capaAnterior := capasDeSueloAExcavar at: index-1 .
			capaActual := capasDeSueloAExcavar at: index .
			(capaAnterior isKindOf: capaActual class) ifTrue: [Perforadora errorNoSePuedenTenerDosCapasContiguasIguales ]
		]
	]

	! !
