calcularProfundidadDelSuelo
	
	| acc |

	acc := 0.
	
	"capasDeSueloDebajo do: [ :capa | 
		| altura |
		capa := capasDeSueloDebajo at: ix.
		altura := capa altura.
		altura = nil ifTrue: [ altura := 0 ].
		acc := acc + altura.
	]."

	capasDeSueloDebajo inject: acc into: [ :capa | 
		| altura |
		altura := capa altura.
		altura = nil ifTrue: [ altura := 0 ].
		acc := acc + altura.
	].

	^ acc.
	
