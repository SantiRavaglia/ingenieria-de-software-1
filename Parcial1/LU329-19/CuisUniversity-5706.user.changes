sobreSuelo: capasDeSueloAExcavar 
	
	| capaAnterior capaActual ix |

	ix := 1 .
	capaAnterior := capasDeSueloAExcavar at: 1 .
	capaActual  := capasDeSueloAExcavar at: 2 .
	
	(capaAnterior = capaActual) & (ix < capasDeSueloAExcavar size) whileFalse: [
		capaAnterior := capasDeSueloAExcavar at: ix-1 .
		capaActual := capasDeSueloAExcavar at:ix .
		ix := ix + 1.
	].

	^self new initializeSobreSuelo: capasDeSueloAExcavar 
	
	"arena tierra arena arena"! !

