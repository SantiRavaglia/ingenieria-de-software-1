calcularProfundidadDelSueloHasta: tipoDeCapa
	
	| valor acc |
	
	valor := 0.
	tipoDeCapa = #Arenoso ifTrue: [ valor := 7 ].
	tipoDeCapa = #Tierra ifTrue: [ valor := 10 ].
	tipoDeCapa = #Concreto ifTrue: [ valor := 50].	
	
	acc := 0.
	
	acc := capasDeSueloDebajo inject: 0 into: [ :capasAcumuladas :capaActual |
		valor = capaActual scan ifTrue: [ ^ acc ].
		capasAcumuladas + capaActual altura 
	].

	^ acc.
	
	 ! !
